{
    "search.exclude": {
        "**/node_modules": false,
        "**/bower_components": true
    },
    "java.home": "/Library/Java/JavaVirtualMachines/jdk1.8.0_151.jdk/Contents/Home",
    "workbench.colorTheme": "Tinacious Design",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "FuraCode Nerd Font",
    "editor.fontLigatures": true,
    "files.autoSave": "afterDelay",
    "workbench.startupEditor": "newUntitledFile",
    "editor.mouseWheelZoom": false,
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.cursorStyle": "block",
    "workbench.colorCustomizations": {
        "editorWhitespace.foreground": "#03b1f141",
        "editorCursor.foreground": "#00ccff",
        "editorCursor.background": "#ff00f2",
        "editorBracketMatch.background": "#ff00dd",
        "editorBracketMatch.border": "#00ffff",
        "editor.lineHighlightBackground": "#444466CC"
    },
    "window.zoomLevel": -1,
    "[javascript]": {
        "editor.formatOnSave": false,
        "editor.insertSpaces": true,
        "prettier.tabWidth": 2,
        "prettier.useTabs": false,
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "prettier.tabWidth": 2,
        "prettier.useTabs": false
    },
    "prettier.singleQuote": true,
    "prettier.trailingComma": "all",
    "editor.insertSpaces": true,
    "prettier.tabWidth": 2,
    "prettier.useTabs": false,

    "explorer.confirmDelete": false,
    "editor.detectIndentation": true,
    "files.trimTrailingWhitespace": true,
    "powermode.enabled": true,
    "powermode.presets": "exploding-rift",
    "powermode.backgroundMode": "image",
    "powermode.explosionDuration": 500,
    "powermode.enableShake": true,
    "powermode.shakeIntensity": 3,
    "powermode.maxExplosions": 1,
    "powermode.comboThreshold": 1,
    "powermode.comboTimeout": 10,

    "vim.easymotion": true,
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": [" "],
            "after": ["leader", "leader", "leader", "b", "d", "w"]
        }
    ],
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "extensions.ignoreRecommendations": false
}
