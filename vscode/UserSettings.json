{
  "workbench.colorTheme": "Snazzy Operator",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "FuraCode Nerd Font",
  "editor.fontLigatures": true,
  "files.autoSave": "afterDelay",
  "workbench.startupEditor": "newUntitledFile",
  "editor.mouseWheelZoom": false,
  //  "workbench.panel.location": "bottom",
  "explorer.confirmDragAndDrop": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.cursorStyle": "block",
  "workbench.colorCustomizations": {
      "editorWhitespace.foreground": "#03b1f141",
      "editorCursor.foreground": "#00ccff",
      "editorCursor.background": "#ff00f2",
      "editorBracketMatch.background": "#ff00dd",
      "editorBracketMatch.border": "#00ffff",
      "editor.lineHighlightBackground": "#444466CC",
      //    "titleBar.border": "#2b9de9",
      //    "list.activeSelectionBackground": "#05080A",
      //    "list.inactiveSelectionBackground": "#05080A",
      //    "tab.inactiveBackground": "#05080A",
      //    "editorGroupHeader.tabsBackground": "#05080A",
      //    "editorGroupHeader.noTabsBackground": "#05080A",
      //    "activityBar.background": "#05080A",
      //    "sideBar.background": "#05080A",
      //    "sideBarSectionHeader.background": "#05080A",
      //    "activityBar.border": "#0b1015",
      //    "tab.border": "#0b1015",
      //    "panel.border": "#05080A",
      //    "statusBar.border": "#05080A"
  },
  "window.zoomLevel": -1,
  "[javascript]": {
      "editor.formatOnSave": false,
      "editor.insertSpaces": false,
      "prettier.tabWidth": 4,
      "prettier.useTabs": true,
      "editor.tabSize": 4
  },
  "[json]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2,
      "prettier.tabWidth": 2,
      "prettier.useTabs": false,
  },
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.tabWidth": 4,
  "prettier.useTabs": true,
  "explorer.confirmDelete": false,
  "editor.detectIndentation": false,
  "files.trimTrailingWhitespace": true,
  "powermode.enabled": true,
  "powermode.presets": "fireworks",
  "powermode.backgroundMode": "image",
  "powermode.explosionDuration": 500,
  "powermode.customExplosions": [
