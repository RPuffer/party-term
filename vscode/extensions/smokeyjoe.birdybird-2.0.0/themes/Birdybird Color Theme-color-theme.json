{
	"name": "Birdybird Color Theme",
    "colors": {
		"editor.background": "#0C0C0C",
		"editor.foreground": "#00CCFF",
		"activityBar.background": "#0C0C0C",
		"activityBar.foreground": "#ffffff",
		"activityBarBadge.background": "#FF009D",
		"sideBar.background": "#141414",
		"tab.inactiveBackground": "#141414",
		"tab.activeBackground": "#0C0C0C",
		"tab.border": "#0C0C0C",
		"statusBar.background": "#0069ff"
	},
	"tokenColors": [
		{
			"settings": {
				"background": "#101226",
				"foreground": "#00CCFF"
			}
		},
		{
			"name": "Meta Function Call",
			"scope": "meta.function-call",
			"settings": {
				"foreground": "#FF009D"
			}
		},
		{
			"name": "Embedded",
			"scope": "punctuation.section.embedded",
			"settings": {
				"foreground": "#D80056"
			}
		},
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#BB00FF"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#BEDBE0"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FF009D"
			}
		},
		{
			"name": "Built-in constants",
			"scope": "constant.language",
			"settings": {
				"foreground": "#FF009D"
			}
		},
		{
			"name": "User-defined constants",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#FF009D"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#FFF600"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#00CCFF"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#00CCFF"
			}
		},
		{
			"name": "Storage types",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00CCFF"
			}
		},
		{
			"name": "Class names",
			"scope": [
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#ffeebb"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#ddbb88"
			}
		},
		{
			"name": "Function names",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#28D367"
			}
		},
		{
			"name": "Function arguments",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#F2B42E"
			}
		},
		{
			"name": "Tag names",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#E500FF"
			}
		},
		{
			"name": "Tag attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#26C5FF"
			}
		},
		{
			"name": "Library functions",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF009D"
			}
		},
		{
			"name": "Library constants",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF009D"
			}
