                "bracketContentsForeground": "#c0ccdb",
                "bracketContentsOptions": "underline",
                "tagsOptions": "stippled_underline"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#4e5c66"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#84c4ce"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#529ca8"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#d4856a"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
