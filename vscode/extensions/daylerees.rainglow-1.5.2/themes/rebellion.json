            "settings": {
                "foreground": "#988f2a"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#fe5f00"
            }
        },
        {
            "name": "Storage",
