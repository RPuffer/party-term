{
    "name": "darkside-light",
    "type": "light",
    "tokenColors": [
        {
            "settings": {
                "background": "#e0e0e0",
                "foreground": "#222324",
                "selectionForeground": "#ffffff",
                "findHighlight": "#ffe792",
                "findHighlightForeground": "#333333",
                "selectionBorder": "#ffffff",
                "bracketsForeground": "#222324",
                "bracketsOptions": "underline",
                "bracketContentsForeground": "#222324",
                "bracketContentsOptions": "underline",
                "tagsOptions": "stippled_underline"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#494b4d"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#4b8e30"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#7b5baf"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#bf2713"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#c1721d"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "",
                "foreground": "#15a2c1"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "",
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#4b8e30"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "",
                "foreground": "#222324"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#15a2c1"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#7b5baf"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#664e4d",
                "foreground": "#cf433e",
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#664e4d",
                "foreground": "#cf433e"
            }
        },
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#4b8e30"
            }
        },
        {
            "name": "diff.header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#494b4d"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#e61f44"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#A6E22E"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#f7b83d"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#8FBE00A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#242526"
            }
        },
        {
            "scope": [
                "meta.property-value",
                "support.constant.property-value",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#4b8e30"
            }
        },
        {
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#bf2713"
            }
        },
        {
            "scope": "meta.structure.dictionary.value.json string.quoted.double.json",
            "settings": {
                "foreground": "#4b8e30"
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-name support.type.property-name",
            "settings": {
                "fontStyle": "normal"
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-value punctuation.separator.key-value",
            "settings": {
                "foreground": "#222324"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "keyword.other.use",
                "keyword.other.function.use",
                "keyword.other.namespace",
                "keyword.other.new",
                "keyword.other.special-method",
                "keyword.other.unit",
                "keyword.other.use-as"
            ],
            "settings": {
                "foreground": "#15a2c1"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "meta.use support.class.builtin",
                "meta.other.inherited-class support.class.builtin"
            ],
            "settings": {
                "foreground": "#15a2c1",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.other",
            "settings": {
                "foreground": "#222324",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Coffeescript Function argument",
            "scope": "variable.parameter.function.coffee",
            "settings": {
                "foreground": "#7b5baf",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown Titles",
            "scope": "entity.name.section.markdown",
            "settings": {
                "foreground": "#bf2713"
            }
        },
        {
            "name": "Markdown Title Hash",
            "scope": "punctuation.definition.heading.markdown",
            "settings": {
                "foreground": "#c1721d"
            }
        },
        {
            "name": "Markdown Raw",
            "scope": "markup.raw.inline.markdown",
            "settings": {
                "foreground": "#4b8e30"
            }
        },
        {
            "name": "Markdown bold stars",
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "foreground": "#c1721d"
            }
        },
        {
            "name": "Markdown link title braces",
            "scope": [
                "punctuation.definition.string.begin.markdown",
                "punctuation.definition.string.end.markdown"
            ],
            "settings": {
                "foreground": "#c1721d"
            }
        },
        {
            "name": "Markdown link braces",
            "scope": "punctuation.definition.metadata.markdown",
            "settings": {
                "foreground": "#c1721d"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown",
                "meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#15a2c1",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": [
                "markup.bold.markdown",
                "markup.italic.markdown"
            ],
            "settings": {
                "foreground": "#15a2c1"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown pre",
            "scope": "markup.raw.block.markdown",
            "settings": {
                "foreground": "#664e4d"
            }
        },
        {
            "name": "GitGutter deleted",
            "scope": "markup.deleted.git_gutter",
            "settings": {
                "foreground": "#e61f44"
            }
        },
        {
            "name": "GitGutter inserted",
            "scope": "markup.inserted.git_gutter",
            "settings": {
                "foreground": "#a7da1e"
            }
        },
        {
            "name": "GitGutter changed",
            "scope": "markup.changed.git_gutter",
            "settings": {
                "foreground": "#f7b83d"
            }
        },
        {
            "name": "Template expressions.",
            "scope": "meta.template.expression",
            "settings": {
                "foreground": "#222324"
            }
        }
    ],
