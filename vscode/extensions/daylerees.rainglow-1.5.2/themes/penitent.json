            "settings": {
                "foreground": "#ea983a"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#e54334"
            }
        },
        {
            "name": "Storage",
